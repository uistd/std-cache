{
  "name": "uistd/cache",
  "description": "UI Service 框架缓存组件，定义了缓存操作接口，实现多种缓存方式。",
  "keywords": [
    "ui service",
    "uistd",
    "cache"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "uistd/event": "3.*",
    "uistd/common": "3.*",
    "uistd/logger": "2.*",
    "uistd/console": "2.*"
  },
  "autoload": {
    "psr-4": {
      "UiStd\\Cache\\": "src"
    }
  },
  "minimum-stability": "stable",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  }
}
